import pandas as pd
import sqlite3
from sqlalchemy import create_engine

data_url = 'https://gist.githubusercontent.com/gbeo/8d682e81d6f1c711a361/raw/d16c708b3c0f65df1c53e0209c13affc29f60b09/vgsales.csv'

try:
    df = pd.read_csv(data_url)
    print("Data loaded successfully!")
except Exception as e:
    print(f"Error loading data: {e}")

engine = create_engine('sqlite:///vgsales.db', echo=False)
conn = engine.raw_connection()
cursor = conn.cursor()

df.to_sql('sales', conn, if_exists='replace', index=False)

print("Data successfully saved to vgsales.db in the 'sales' table.")
